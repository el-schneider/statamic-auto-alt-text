        "pint:check": "./vendor/bin/pint --test",
        "pint:fix": "./vendor/bin/pint"
      },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.2.1",
        "prettier": "^3.5.0",
        "prettier-plugin-organize-imports": "^4.1.0",
        "vite": "^6.1.0"
    },
    "dependencies": {
        "@ai-sdk/openai": "^1.1.12",
        "@genkit-ai/dotprompt": "^0.9.12",
        "ai": "^4.1.41",
        "dotprompt": "^1.0.1",
        "i": "^0.3.7",
        "npm": "^11.1.0"
    }
}
