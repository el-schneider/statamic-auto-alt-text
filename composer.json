{
    "name": "el-schneider/statamic-auto-alt-text",
    "autoload": {
        "psr-4": {
            "ElSchneider\\StatamicAutoAltText\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^6.0",
        "prism-php/prism": "^0.99"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.22",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-browser": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Auto Alt Text",
            "description": "Statamic Auto Alt Text addon"
        },
        "laravel": {
            "providers": [
                "ElSchneider\\StatamicAutoAltText\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
